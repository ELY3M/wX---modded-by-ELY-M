<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:ctc="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:layout_above="@id/toolbar_bottom"
    android:orientation="vertical">

    <include layout="@layout/widget_toolbar_top" />

    <ScrollView
        android:id="@+id/sv"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:scrollbars="none">

        <LinearLayout
            android:id="@+id/ll"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:padding="0dp"
            android:stretchColumns="*">

            <joshuatee.wx.util.ImageMap
                android:id="@+id/map_ab"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                ctc:map="AB" />

            <joshuatee.wx.util.ImageMap
                android:id="@+id/map_bc"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                ctc:map="BC" />

            <joshuatee.wx.util.ImageMap
                android:id="@+id/map_mb"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                ctc:map="MB" />

            <joshuatee.wx.util.ImageMap
                android:id="@+id/map_nb"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                ctc:map="NB" />

            <joshuatee.wx.util.ImageMap
                android:id="@+id/map_nl"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                ctc:map="NL" />

            <joshuatee.wx.util.ImageMap
                android:id="@+id/map_ns"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                ctc:map="NS" />

            <joshuatee.wx.util.ImageMap
                android:id="@+id/map_nt"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                ctc:map="NT" />

            <joshuatee.wx.util.ImageMap
                android:id="@+id/map_nu"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                ctc:map="NU" />

            <joshuatee.wx.util.ImageMap
                android:id="@+id/map_on"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                ctc:map="ON" />

            <joshuatee.wx.util.ImageMap
                android:id="@+id/map_pe"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                ctc:map="PE" />

            <joshuatee.wx.util.ImageMap
                android:id="@+id/map_qc"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                ctc:map="QC" />

            <joshuatee.wx.util.ImageMap
                android:id="@+id/map_sk"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                ctc:map="SK" />

            <joshuatee.wx.util.ImageMap
                android:id="@+id/map_yt"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                ctc:map="YT" />

        </LinearLayout>
    </ScrollView>
</LinearLayout>



    
